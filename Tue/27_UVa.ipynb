   "source": [
    "```cpp\n",
    "#include <iostream>\n",
    "using namespace std;\n",
    "\n",
    "int main()\n",
    "{\n",
    "    int n;\n",
    "    while (cin >> n, n)\n",
    "    {\n",
    "        while (n >= 10)\n",
    "        {\n",
    "            int ds = 0;\n",
    "            while (n) ds += n % 10, n /= 10;\n",
    "            n = ds;\n",
    "        }\n",
    "        cout << n << endl;\n",
    "    }\n",
    "}\n",
    "```"
   ]
