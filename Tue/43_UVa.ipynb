   "source": [
    "```cpp\n",
    "#include <iostream>\n",
    "#include <string>\n",
    "#include <map>\n",
    "using namespace std;\n",
    "\n",
    "int main()\n",
    "{\n",
    "    map<string, int> msi;\n",
    "    string s, t;\n",
    "    cin >> s;\n",
    "    while (cin >> s && getline(cin, t))\n",
    "        msi[s]++;\n",
    "    for (auto &kv : msi) cout << kv.first << \" \" << kv.second << endl;\n",
    "}\n",
    "```"
   ]
  }
